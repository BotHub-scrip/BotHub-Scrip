_G.Auto_Fram = true
_G.ByPass_Tween = true
function Warp_Tween(P)
    local Pos = (P.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
    local tween_Service = game:GetService("TweenService")
    local tween_info = TweenInfo.new(Pos/370, Enum.EasingStyle.Linear)
    local Warps = tween_Service:Create(game:GetService("Players").LocalPlayer.Character["HumanoidRootPart"], tween_info, {CFrame = P})
    Warps:Play()
    if Pos <= 50 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = P
    end
end
function ByPassTween()
    pcall(function()
        if _G.ByPass_Tween then
            CheckLevel()
            repeat
                if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value ~= SpawnPoint then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFramePuk
                    game.Players.LocalPlayer.Character.Humanoid.Health = 0
                end
            until _G.ByPass_Tween == false or game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value == SpawnPoint
        else
            Warp_Tween(CFramePuk)
        end
    end)
end
function CheckLevel()
    local Level = game:GetService("Players").LocalPlayer.Data.Level.Value
    if Level == 1 or Level <= 9 then
        Mon = "Bandit"
        SpawnPoint = "Default"
        QuestName = "BanditQuest1"
        QuestNumber = 1
        CFrameQuest = CFrame.new(1059.9759521484375, 16.488065719604492, 1549.023193359375)
        CFramePuk = CFrame.new(1108.1478271484375, 57.41001510620117, 1623.7769775390625)
        CheckMon = {
            Mon1 = CFramePuk,
            Mon2 = CFramePuk,
            Mon3 = CFramePuk,
            Mon4 = CFramePuk,
            Mon5 = CFramePuk,
        }
    elseif Level == 10 or Level <= 14 then
        Mon = "Monkey"
        SpawnPoint = "Jungle"
        QuestName = "JungleQuest"
        QuestNumber = 1
        CFrameQuest = CFrame.new(-1599.5264892578125, 36.977455139160156, 153.2390899658203)
        CFramePuk = CFrame.new(-1586.5660400390625, 39.97743606567383, 155.67477416992188)
        CheckMon = {
            Mon1 = CFramePuk,
            Mon2 = CFramePuk,
            Mon3 = CFramePuk,
            Mon4 = CFramePuk,
            Mon5 = CFramePuk,
        }
    end
end
-- Warp ByPass_Tween
spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
        if _G.Auto_Fram then
            if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
                CheckLevel()
                if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value ~= SpawnPoint then
                    ByPassTween()
                else
                    Warp_Tween(CFrameQuest)
                end
                if (CFrameQuest.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 50 then
                    wait(.1)
                    local args = {
                        [1] = "StartQuest",
                        [2] = QuestName,
                        [3] = QuestNumber
                    }
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                end
            end
        end
    end)
end)
-- Mon
spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
        if _G.Auto_Fram then
            pcall(function()
                CheckLevel()
                for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                    if v.Name == Mon then
                        if v.Humanoid.Health == 0 then
                            v:Destroy()
                        end
                    end
                end
            end)
        end
    end)
end)
spawn(function()
    while wait() do
        if _G.Bring then
            pcall(function()
                for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                    for x,y in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        CheckLevel()
                        if v.Name == Mon and y.Name == Mon then
                            v.HumanoidRootPart.CFrame = y.HumanoidRootPart.CFrame
                            v.HumanoidRootPart.CanCollide = false
                            y.HumanoidRootPart.CanCollide = false
                            v.HumanoidRootPart.Size = Vector3.new(150, 50, 150)
                            y.HumanoidRootPart.Size = Vector3.new(150, 50, 150)
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                        end
                    end
                end
            end)
        end
    end
end)
-- Anchored
spawn(function()
    while wait() do
        if _G.Auto_Fram then
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Anchored = true
        else
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Anchored = false
        end
    end
end)
spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
        if _G.Auto_Fram then
            if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
                CheckLevel()
                pcall(function()
                    for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        for x,y in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                            if v.Name == Mon and y.Name == Mon then
                                Warp_Tween(v.HumanoidRootPart.CFrame * CFrame.new(0, 18, 0))
                                _G.Bring = true
                            elseif game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value ~= SpawnPoint then
                                ByPassTween()
                                _G.Bring = false
                            elseif game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value == SpawnPoint then
                                Warp_Tween(CheckMon.Mon1)
                                _G.Bring = false
                                if (CheckMon.Mon1.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 20 then
                                    Warp_Tween(CheckMon.Mon2)
                                end
                            end
                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                        end
                    end
                end)
            end
        end
    end)
end)

-- Attack
spawn(function()
    local StopCamera = require(game.ReplicatedStorage.Util.CameraShaker)
    StopCamera:Stop()
    for i,y in pairs(getreg()) do
        if typeof(y) == "function" and getfenv(y).script == game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework then
            for x,v in pairs(debug.getupvalues(y)) do
                spawn(function()
                    pcall(function()
                        sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                        v.activeController.hitboxMagnitude = 150
                        v.activeController.timeToNextAttack = -(math.huge^math.huge^math.huge)
                        v.activeController.attacking = false
                        v.activeController.increment = 4
                        v.activeController.blocking = false
                        v.activeController.humanoid.AutoRotate = true
                        v.activeController.focusStart = 0
                        v.activeController.currentAttackTrack = 0
                    end)
                end)
            end
        end
    end
end)
spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
        if _G.Auto_Fram then
            if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
                CheckLevel()
                for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                    if v.Name == Mon then
                        if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 50 then
                            _G.Attack = true
                        else
                            _G.Attack = false
                        end
                    end
                end
            end
        end
    end)
end)
spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
        if _G.Attack then
            game:GetService'VirtualUser':CaptureController()
            game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
        end
    end)
end)
