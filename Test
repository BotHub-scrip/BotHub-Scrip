getfenv().AutoFram = true
getfenv().bypasstween = true
local Mon
local LastSpawnPoint
local CFrameQuest
local QuestName
local QuestNumber
local CFramePuk
local Anchoreds = game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored
local loop = game:GetService("RunService").RenderStepped
function Tween(C)
    CheckLevel()
    Anchoreds = true
    local Pos = (C.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
    local tween_Service = game:GetService("TweenService")
    local tween_info = TweenInfo.new(Pos/365, Enum.EasingStyle.Linear)
    local Warps = tween_Service:Create(game:GetService("Players").LocalPlayer.Character["HumanoidRootPart"], tween_info, {CFrame = C})
    Warps:Play()
    if Pos <= 30 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = C
        if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value == LastSpawnPoint then
            Anchoreds = false
        end
    end
end
function CheckLevel()
    local Level = game:GetService("Players").LocalPlayer.Data.Level.Value
    if Level < 10 then
        Mon = "Bandit"
        LastSpawnPoint = "Default"
        QuestName = "BanditQuest1"
        QuestNumber = 1
        CFrameQuest = CFrame.new(1059.9759521484375, 16.488065719604492, 1549.023193359375)
        CFramePuk = CFrame.new(1108.1478271484375, 57.41001510620117, 1623.7769775390625)
    elseif Level < 15 then
        Mon = "Monkey"
        LastSpawnPoint = "Jungle"
        QuestName = "JungleQuest"
        QuestNumber = 1
        CFrameQuest = CFrame.new(-1599.5264892578125, 36.977455139160156, 153.2390899658203)
        CFramePuk = CFrame.new(-1586.5660400390625, 39.97743606567383, 155.67477416992188)
    end
end
spawn(function()
    loop:Connect(function()
        if getfenv().AutoFram then
            pcall(function()
                if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
                    CheckLevel()
                    if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value == LastSpawnPoint then
                        if (CFrameQuest.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 50 then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameQuest
                            wait(.1)
                            local args = {
                                [1] = "StartQuest",
                                [2] = QuestName,
                                [3] = QuestNumber
                            }
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                        else
                            Tween(CFrameQuest)
                        end
                    else
                        if getfenv().bypasstween then
                            repeat
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameQuest
                                game.Players.LocalPlayer.Character.Humanoid.Health = 0
                            until getfenv().bypasstween == false or game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value == LastSpawnPoint
                        else
                            Tween(CFrameQuest)
                        end
                    end
                end
            end)
        end
    end)
end)
spawn(function()
    loop:Connect(function()
        if getfenv().AutoFram then
            pcall(function()
                CheckLevel ()
                if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
                    if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value == LastSpawnPoint then
                        for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                            for x,y in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                                if v.Name == Mon and y.Name == Mon then
                                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Anchored = true
                                    v.HumanoidRootPart.CFrame = y.HumanoidRootPart.CFrame
                                    v.HumanoidRootPart.Size = Vector3.new(60,60,60)
                                    v.HumanoidRootPart.CanCollide = false
                                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BotHub-scrip/BotHub-Scrip/main/Simulation"))()
                                    Tween(v.HumanoidRootPart.CFrame * CFrame.new(0,15,0))
                                    if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 50 then
                                        getfenv().Attack = true
                                    else
                                        getfenv().Attack = false
                                    end
                                else
                                    Tween(CFramePuk)
                                end
                            end
                        end
                    end
                end
            end)
        end
    end)
end)
spawn(function()
    loop:Connect(function()
        if getfenv().AutoFram then
            pcall(function()
                CheckLevel()
                for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                    for x,y in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if v.Name == Mon and y.Name == Mon then
                            if v.Humanoid.Health == 0 then
                                wait(0.1)
                                v:Destroy()
                            end
                        end
                    end
                end
            end)
        end
    end)
end)
spawn(function()
    loop:Connect(function()
        if getfenv().AutoFram then
            pcall(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/BotHub-scrip/BotHub-Scrip/main/FastAttack"))()
            end)
        end
    end)
end)
spawn(function()
    loop:Connect(function()
        if getfenv().Attack then
            game:GetService'VirtualUser':CaptureController()
            game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
        end
    end)
end)
